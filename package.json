{
  "name": "scribe-web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": "22.15.x"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx,js,jsx --cache --no-error-on-unmatched-pattern",
    "lint:fix": "eslint . --ext ts,tsx,js,jsx --fix --cache --no-error-on-unmatched-pattern",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest run",
    "deps:clean": "pnpm prune && pnpm install",
    "deps:reset": "rm -rf node_modules && rm pnpm-lock.yaml && pnpm install",
    "typecheck": "tsc --noEmit",
    "precheck": "pnpm lint && pnpm format:check && pnpm typecheck && pnpm test",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "set-env": "node scripts/set-env.ts"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.3.0",
    "@microsoft/applicationinsights-web": "^3.3.8",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-router": "^1.120.2",
    "@tanstack/react-router-devtools": "^1.120.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^25.2.0",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.508.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.1",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.4",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tailwindcss/postcss": "^4.1.6",
    "@tailwindcss/vite": "^4.1.6",
    "@tanstack/router-plugin": "^1.120.2",
    "@types/node": "^20.17.45",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.0.11",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^4.1.6",
    "typescript": "5.5.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^5.4.19",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --cache --no-error-on-unmatched-pattern",
      "prettier --write"
    ],
    "*.{jsx,mjs}": [
      "eslint --fix --cache --no-error-on-unmatched-pattern",
      "prettier --write"
    ],
    "*.{cjs,js}": [
      "prettier --write"
    ],
    "*.{json,css,md,html,yml,yaml}": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide"
    ]
  }
}
